{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/bm-app/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|map|ico|png|svg|woff2?)$",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
